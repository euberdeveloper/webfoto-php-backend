{
    "name": "euber/webfoto-php-backend",
    "description": "The php backend of webfoto",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Euber\\WebfotoPhpBackend\\": "src/"
        }
    },
    "authors": [
        {
            "name": "euberdeveloper",
            "email": "euberdeveloper@gmail.com"
        }
    ],
    "require": {
        "wherw/scan-path": "^1.0",
        "webmozart/path-util": "^2.3",
        "myclabs/php-enum": "^1.8",
        "vlucas/phpdotenv": "^5.3",
        "propel/propel": "~2.0@dev"
    },
    "scripts": {
        "start": "php -S localhost:4000 index.php",
        "propel:addLn": "ln -s vendor/bin/propel propel",
        "propel:sql": "propel sql:build --config-dir ./src/db --schema-dir ./src/db ",
        "propel:model": "propel model:build --config-dir ./src/db --schema-dir ./src/db "
    }
}
